{
    "name": "misfist/littlesis-packages",
    "description": "WordPress packages for blog.littlesis.org",
    "type": "project",
    "license": "GPLv3",
    "authors": [
        {
            "name": "Pea",
            "email": "pea@misfist.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/littlesis",
                "version": "v0.1.6",
                "type": "wordpress-theme",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/littlesis/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "git@github.com:misfist/littlesis.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "misfist/littlesis-core-functionality",
                "version": "v0.1.3",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/littlesis-core-functionality/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/littlesis-core-functionality.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "misfist/advanced-custom-fields-pro",
                "version": "v5.5.7",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/misfist/advanced-custom-fields/archive/master.zip"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/misfist/advanced-custom-fields.git",
                    "reference": "master"
                }
            }
        }


    ],
    "require": {
      "misfist/advanced-custom-fields-pro": "v5.5.7",
      "wpackagist-plugin/caldera-forms": "1.5.0",
      "wpackagist-plugin/co-authors-plus": "^3.2.1",
      "wpackagist-plugin/codepress-admin-columns": "^2.5.6.4",
      "wpackagist-plugin/jetpack": "^4.3.2",
      "wpackagist-plugin/series": "^1.0.0",
      "wpackagist-plugin/shortcode-ui": "^0.7.0",
      "wpackagist-plugin/widget-css-classes": "^1.4.0",
      "wpackagist-plugin/wp-user-avatars": "^1.0.2",
      "misfist/littlesis-core-functionality": "v0.1.3",
      "wpackagist-theme/understrap": "^0.5.6",
      "misfist/littlesis": "v0.1.6"
    },
    "require-dev": {
      "wpackagist-plugin/developer": "^1.2.5",
      "wpackagist-plugin/term-management-tools": "^1.1.4",
      "wpackagist-plugin/transients-manager": "^1.7.3",
      "wpackagist-plugin/user-switching": "^1.0.9",
      "wpackagist-plugin/which-template": "^4.0",
      "wpackagist-plugin/wordpress-importer": "^0.6.3",
      "wpackagist-plugin/wp-log-viewer": "^1.0.7"
    },
    "extra": {
      "installer-paths": {
        "../wordpress/wp-content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
        "../wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
        "../wordpress/wp-content/themes/{$name}/": ["type:wordpress-theme"]
      }
    }
}
